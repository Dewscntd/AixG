    "@hookform/resolvers": "^3.3.0",
    "zod": "^3.22.0",
    "react-intersection-observer": "^9.5.0",
    "react-window": "^1.8.0",
    "react-window-infinite-loader": "^1.0.0",
    "react-virtualized-auto-sizer": "^1.0.0",
    "graphql": "^16.8.0",
    "graphql-request": "^6.1.0",
    "graphql-ws": "^5.14.0",
    "ws": "^8.14.0",

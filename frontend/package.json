{
  "name": "footanalytics-frontend",
  "version": "1.0.0",
  "description": "FootAnalytics Frontend - Scalable micro-frontend architecture with Next.js 14",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "analyze": "cross-env ANALYZE=true next build",
    "codegen": "graphql-codegen --config codegen.yml",
    "i18n:extract": "formatjs extract",
    "i18n:compile": "formatjs compile",
    "pwa:build": "next build && next-pwa",
    "lighthouse": "lhci autorun"
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.0",
